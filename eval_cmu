#!/bin/sh

if [[ "$1" == "1" ]]; then
  echo "Using small dataset"

  python3 train.py \
    --eval --eval_dataset val \
    --config experiments/cmupanoptic/eval/cmu_vol_softmax_small.yaml \
    --logdir ./logs
else
  echo "Using main dataset"

  python3 train.py \
    --eval --eval_dataset val \
    --config experiments/cmupanoptic/eval/cmu_vol_softmax.yaml \
    --logdir ./logs
fi

